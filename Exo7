### Programme qui execute le programme d'avant

#include<unistd.h>
#include<sys/types.h>
#include<sys/wait.h> 

int main(int argc, char **argv) {
	execve("/home/villebon/Bureau/Exo7", NULL, NULL); 
	return 0;
}



